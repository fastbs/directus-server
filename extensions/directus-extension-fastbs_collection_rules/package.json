{
	"name": "fastbs_collection_rules",
	"description": "Custom rules for Directus collections",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"type": "module",
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "module",
				"name": "fastbs.module",
				"source": "src/fastbs.module/index.ts"
			},
			{
				"type": "hook",
				"name": "fastbs.hook",
				"source": "src/fastbs.hook/index.js"
			},
			{
				"type": "interface",
				"name": "fastbs.interface.collection.selector",
				"source": "src/fastbs.interface.collection.selector/index.ts"
			}
		],
		"host": "^10.3.3"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/errors": "^0.2.3",
		"@directus/extensions-sdk": "10.3.3",
		"@types/node": "^20.11.19",
		"sass": "^1.71.1",
		"typescript": "^5.3.3",
		"vue": "^3.4.19"
	},
	"dependencies": {
		"lodash.get": "^4.4.2"
	}
}
